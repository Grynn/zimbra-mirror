                    <object class="com.install4j.api.beans.ScriptProperty">
                      <void property="value">
                        <string>try {
    double oldVersion = Double.parseDouble(context.getCompilerVariable("sys.version"));

    String var = java.util.prefs.Preferences.userNodeForPackage(java.util.prefs.Preferences.class).get("zdesktop.wipeIfBefore", null);
    if (var == null || var.length() == 0)
        return Boolean.FALSE;
    
    double wipeIfBefore = Double.parseDouble(var);
    
    if (oldVersion &gt; 0 &amp;&amp; wipeIfBefore &gt; 0 &amp;&amp; oldVersion &lt; wipeIfBefore)
        return Boolean.TRUE;

} catch (Throwable t) {}

return Boolean.FALSE;</string>
