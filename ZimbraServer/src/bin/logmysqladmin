source `dirname $0`/zmshutil || exit 1
zmsetvars zimbra_home logger_mysql_socket mysql_logger_root_password

exec ${zimbra_home}/logger/mysql/bin/mysqladmin -S ${logger_mysql_socket} -u root --password=${mysql_logger_root_password} "$@"
