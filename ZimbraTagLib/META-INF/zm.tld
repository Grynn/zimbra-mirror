            </description>
            <name>strArr</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>
	
	<attribute>
            <description>
                Map to edit
            </description>
            <name>map</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.util.HashMap</type>
        </attribute>

	<attribute>
            <description>
                Value to add
            </description>
            <name>add</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
            <type>java.lang.String</type>
        </attribute>

	<attribute>
            <description>
