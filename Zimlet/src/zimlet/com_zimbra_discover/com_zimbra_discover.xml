<zimlet name="com_zimbra_discover" version="0.1" description="Discover!">
  <include>discover.js</include>
  <includeCSS>discover.css</includeCSS>
  <handlerObject>com_zimbra_discover</handlerObject>

  <zimletPanelItem label="discover!" icon="dy-panelIcon">
    <toolTipText>Discover websites based on topics of your choice</toolTipText>
  </zimletPanelItem>

  <userProperties>
	<property type="string" name="dy_usingFirstTime" value="true"/>
	<property type="string" name="dy_discoverBtnClickedOnExternalWindow" value="false"/>
	<property type="string" name="turnONDiscoverZimlet" value="true"/>

   <property type="string" name="dy_pref_art" value="false"/>
   <property type="string" name="dy_pref_business" value="false"/>
   <property type="string" name="dy_pref_entertainment" value="false"/>
   <property type="string" name="dy_pref_humor" value="false"/>
   <property type="string" name="dy_pref_politics" value="false"/>
   <property type="string" name="dy_pref_technology" value="false"/>
   <property type="string" name="dy_pref_sports" value="false"/>
</userProperties>

</zimlet>
