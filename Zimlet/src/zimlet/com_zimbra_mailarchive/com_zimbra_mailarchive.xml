<zimlet name="com_zimbra_mailarchive"
        version="0.6"
        label="${msg.label}"
        description="${msg.description}"
        target="main view-window">
    <include>mailarchive.js</include>
    <includeCSS>mailarchive.css</includeCSS>
    <handlerObject>ZmArchiveZimlet</handlerObject>
    <zimletPanelItem label="${msg.label}" icon="archiveZimletIcon">
        <toolTipText>${msg.description}</toolTipText>
    </zimletPanelItem>
</zimlet>
