<zimlet name="com_zimbra_searchauto"
        version="2.1"
        label="Search History"
        description="Helps autocomplete prior searches">
    <include>searchauto.js</include>
    <includeCSS>searchauto.css</includeCSS>
    <handlerObject>ZmSearchAutoZimlet</handlerObject>
    <userProperties>
        <property type="string" name="history"  value=""/>
        <property type="string" name="turnONAutoComplete" value="false"/>
    </userProperties>
</zimlet>
