<zimlet name="com_zimbra_skinchanger"
        version="1.6"
        label="${msg.label}"
        description="${msg.description}">
	<include>skinchanger.js</include>
	<includeCSS>skinchanger.css</includeCSS>
	<resource>skinchanger-icon.png</resource>
	<handlerObject>com_zimbra_skinchanger_HandlerObject</handlerObject>
	<zimletPanelItem label="${msg.SkinChangerZimlet_panel_label}" icon="skinchanger-panelIcon">
		<toolTipText>${msg.SkinChangerZimlet_panel_tooltip}</toolTipText>
	</zimletPanelItem>
	<userProperties>
		<property type="string" name="turnONSkinChangerZimletNew" value="false"/>
		<property type="string" name="skinc_selectedFreq" value="Wednesday"/>
		<property type="string" name="skinc_skinWasChangedOnDate" value=""/>
	</userProperties>	
</zimlet>
