<zimlet name="com_zimbra_undosend" version="0.2" description="Provides option to cancel or undo mail send for few seconds">
	<include>undosend.js</include>
	<includeCSS>undosend.css</includeCSS>
	<handlerObject>Com_Zimbra_UndoSendHdlr</handlerObject>

  <zimletPanelItem label="${msg.UndoSendZimlet_Label}" icon="Send">
  </zimletPanelItem>

  <userProperties>
		<property type="string" name="undeSend_howMuchDelay" value="5"/>	
  </userProperties>
</zimlet>

